<%= simple_form_for @subscription, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :customer_id %>
  <%= f.input :category_id %>
  <%= f.input :status %>
  <div class="form-actions">
    <%= f.button :submit, :class => 'btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                subscription_path, :class => 'btn' %>
  </div>
<% end %>
